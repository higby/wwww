{
  "name": "higby.io",
  "version": "3.0.0",
  "description": "A place for my ramblings.",
  "scripts": {
    "eleventy-serve": "npx @11ty/eleventy --serve",
    "eleventy": "npx @11ty/eleventy",
    "clean": "rimraf build",
    "sass-watch": "sass --no-source-map src/assets/css:build/assets/css --watch",
    "sass": "sass --no-source-map src/assets/css:build/assets/css",
    "start": "run-p clean eleventy-serve sass-watch",
    "build": "run-p clean eleventy sass"
  },
  "author": "Branden Higby",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "js-beautify": "^1.14.0",
    "jsdom": "^16.6.0",
    "markdown-it": "^12.1.0",
    "markdown-it-anchor": "^8.1.2",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-image-figures": "^1.2.0",
    "markdown-it-mark": "^3.0.1",
    "moment-timezone": "^0.5.33",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "sass": "^1.36.0",
    "sharp": "^0.28.3"
  },
  "main": ".eleventy.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/higby/higby.io.git"
  },
  "keywords": [
    "blog",
    "eleventy",
    "markdown"
  ],
  "bugs": {
    "url": "https://github.com/higby/higby.io/issues"
  },
  "homepage": "https://github.com/higby/higby.io#readme"
}

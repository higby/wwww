---
layout: base
---
<header>{% include 'header.njk' %}</header>
<main>
  <header>
    {% if title %}<h1>{{ title }}</h1>{% endif %}
    <p>Planted <time datetime='{{ page.date | datetime }}' title='{{ page.date | common }}'>{{ page.date | relative }}</time></p>
    {{ content | toc | safe }}
  </header>
  <article>{{ content | safe }}</article>
</main>
<footer>{% include 'footer.njk' %}</footer>


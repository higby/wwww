<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    
    <title>{% if title %}{{ title }}{% else %}Branden Higby{% endif %}</title>
    <meta name="description" content="{{ description }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="generator" content="{{ eleventy.generator }}">

    {% favicons './src/_includes/favicon.svg', appleIconBgColor='#8a64ff' %}

    <link rel="preload" href="/Inter.woff2" as="font" type="font/woff2" crossorigin>

    <style>{{ site.scss | safe }}{{ scss | safe }}</style>

    <link href="mailto:{{ site.email }}" rel="me">
    <link href="https://indieweb.social/@higby" rel="me">
    <link href="https://github.com/{{ site.github }}" rel="me">

    <link rel="webmention" href="https://webmention.io/higby.io/webmention">
    <link rel="pingback" href="https://webmention.io/higby.io/xmlrpc">

    <link rel="alternate" type="application/rss+xml" title="Higby's Garden" href="http://higby.io/feed.xml">

  </head>
  <body>
    {{ content | safe}}
  </body>
</html>


<head>
  <title>{% if title %}{{ title }}{% else %}{{ site.name }}{% endif %}</title>

  <link rel="preload" href="/fonts/Inter.var.woff2" as="font" type="font/woff2" crossorigin>
  <style>{% renderFile "./src/styles/main.scss" %}{% if stylesheet%}{% renderFile 'src/styles/imports/' + stylesheet %}{% endif %}</style>

  {% if eleventyExcludeFromCollections %}<meta name="robots" content="noindex">{% endif %}

  <meta name="description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name='generator' content='eleventy'>
  <meta name="theme-color" content="#141619">

  {% if eleventyExcludeFromCollections != true %}
  <meta property="og:image" content="https://og.higby.io/{% OG page.url %}/opengraph"/>
  <meta name="twitter:image" content="https://og.higby.io/{% OG page.url %}/opengraph"/>
  <meta name="twitter:card" content="summary_large_image"/>
  <meta name="twitter:description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}"/>
  <meta name="twitter:title" content='{% if title %}{{ title }}{% else %}{{ site.name }}{% endif %}'/>
  <meta name="twitter:creator" content="@higby_"/>
  {% endif %}

  <link rel="canonical" href="https://{{ site.url }}{{ page.url }}">
  <link rel="author" href="/humans.txt"/>
  <link rel='icon' href="/favicon.ico">

</head>